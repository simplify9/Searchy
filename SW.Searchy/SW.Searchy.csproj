<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SimplyWorks.Searchy</PackageId>
    <Product>SimplyWorks.Searchy</Product>
    <Authors>Simplify9</Authors>
    <Company />
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/simplify9/Searchy</PackageProjectUrl>
    <RepositoryUrl>https://github.com/simplify9/Searchy</RepositoryUrl>
    <PackageIcon>icon.png</PackageIcon>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SimplyWorks.ObjectConversion" Version="2.0.0" />
    <PackageReference Include="SimplyWorks.PrimitiveTypes" Version="2.0.4" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
